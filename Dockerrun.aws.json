{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {  
          "name": "client",
          "image": "bratzzie/multi-client",
          "hostname": "client",
          "essential": false
      },
      {  
        "name": "server",
        "image": "bratzzie/multi-server",
        "hostname": "api",
        "essential": false
    },
    {  
        "name": "worker",
        "image": "bratzzie/multi-worker",
        "hostname": "worker",
        "essential": false
    },
    {  
        "name": "client",
        "image": "bratzzie/multi-client",
        "hostname": "client",
        "essential": false
    },
    {  
        "name": "nginx",
        "image": "bratzzie/multi-nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
           {
                "hostPort": 80,
                "containerPort": 80
            }
        ],
        "links": ["client", "server"]
    }
]
}