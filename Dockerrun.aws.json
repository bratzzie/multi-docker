{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {  
          "name": "client",
          "image": "bratzzie/multi-client",
          "hostname": "client",
          "essential": false,
          "memory": 128
      },
      {  
        "name": "server",
        "image": "bratzzie/multi-server",
        "hostname": "api",
        "essential": false,
        "memory": 128
    },
    {  
        "name": "worker",
        "image": "bratzzie/multi-worker",
        "hostname": "worker",
        "essential": false,
        "memory": 128
    },
    {  
        "name": "client",
        "image": "bratzzie/multi-client",
        "hostname": "client",
        "essential": false,
        "memory": 128
    },
    {  
        "name": "nginx",
        "image": "bratzzie/multi-nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
           {
                "hostPort": 80,
                "containerPort": 80
            }
        ],
        "links": ["client", "server"],
        "memory": 128
    }
]
}